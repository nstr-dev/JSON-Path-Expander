{
  "name": "json-path-expander",
  "displayName": "JSON Path Expander",
  "description": "Enter a JSON path and value to auto-expand and insert into the active JSON/JSONC file.",
  "version": "1.0.0",
  "publisher": "nstr-dev",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:jsonc",
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jsonPathExpander.insertPathAndValue",
        "title": "JSON Path Expander: Insert/Expand Value"
      }
    ]
  },
  "dependencies": {
    "jsonc-parser": "^3.2.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.4.0",
    "tslib": "^2.6.2",
    "esbuild": "^0.21.0"
  },
  "scripts": {
    "compile": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --platform=node --external:vscode --external:jsonc-parser --format=cjs",
    "watch": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --platform=node --external:vscode --external:jsonc-parser --format=cjs --watch",
    "package": "npx @vscode/vsce package"
  }
}
